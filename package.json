{
  "type": "module",
  "name": "listing",
  "version": "1.0.0",
  "description": "Okay, let's get started designing schema first.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dev": "tsc -p tsconfig.dev.json && tsc-alias",
    "build:prod": "tsc -p tsconfig.prod.json && tsc-alias",
    "start": "npm run build:prod && node dist/index.js",
    "dev": "npm run lint && npm run format && tsc-watch -p tsconfig.dev.json --onSuccess \"npm run start:alias\"",
    "start:alias": "tsc-alias -p tsconfig.dev.json && nodemon dist/index.js",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run format",
      "npm run lint"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "mongodb": "^5.9.2",
    "nanoid": "^5.0.8",
    "pg": "^8.13.1",
    "pino-pretty": "^13.0.0",
    "rotating-file-stream": "^3.2.5",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin-ts": "^2.10.1",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.0",
    "docsify-auto-sidebar": "^1.0.0",
    "esbuild": "^0.24.0",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "parcel": "^2.13.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.1",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.27.2",
    "typedoc-plugin-markdown": "^4.3.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  }
}
